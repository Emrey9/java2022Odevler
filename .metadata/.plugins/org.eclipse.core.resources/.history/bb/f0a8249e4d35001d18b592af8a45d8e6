
public class LoopDemo {

	public static void main(String[] args) {
		//For
		for (int i = 0; i < 10; i+=4) {
			System.out.println(i);
		}
		System.out.println("For Döngüsü Bitti");
		
		//While
		int i = 10;
		while (i<10) {
			System.out.println(i);
		}
		System.out.println("While Döngüsü Bitti");
	}

}
