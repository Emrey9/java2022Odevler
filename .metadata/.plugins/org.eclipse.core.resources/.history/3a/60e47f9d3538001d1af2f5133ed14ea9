
public class arkadasSayilar {

	public static void main(String[] args) {
		int number1 = 6;
		int total1 = 0;
		
		int number2 = 6;
		int total2 = 0;
		
		for (int i = 0; i < number1; i++) {
			if (number1 % i == 0) {
				total1+=i;
			}
		}
		
		for (int i = 0; i < number2; i++) {
			if (number2 % i == 0) {
				total2+=i;
			}
		}

	}

}
